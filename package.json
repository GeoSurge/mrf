{
  "name": "mrf",
  "version": "0.0.0",
  "description": "Read Meta Raster Format Files (e.g. .mrf, .idx, .data)",
  "main": "src/",
  "dependencies": {
    "cross-fetch": "^3.0.5",
    "fast-bin": "^0.1.0",
    "hasha": "^5.2.0",
    "lerc": "^2.0.0",
    "lodash.groupby": "^4.6.0",
    "xml-utils": "^0.2.0"
  },
  "devDependencies": {
    "ava": "^3.11.1",
    "aws-sdk": "^2.727.1",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "finalhandler": "^1.1.2",
    "jpeg-js": "^0.4.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "^5.0.0",
    "serve-static": "^1.14.1",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "webpack",
    "lint": "node ./node_modules/eslint/bin/eslint.js src",
